<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Tooltip?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.shape.Ellipse?>
<?import javafx.scene.text.Font?>

<fx:root type="StackPane" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Ellipse fill="#0039a1" radiusX="10.0" radiusY="10.0" stroke="#00008a" strokeType="INSIDE" strokeWidth="3.0" />
        <Label alignment="CENTER" contentDisplay="CENTER" ellipsisString="" graphicTextGap="0.0" onMouseClicked="#showTip" text="i" textAlignment="CENTER" textFill="WHITE">
            <tooltip>
                <Tooltip fx:id="tip" text="Empty Tooltip" />
            </tooltip>
            <font>
                <Font size="16.0" />
            </font>
         <padding>
            <Insets bottom="-2.0" top="-3.0" />
         </padding>
        </Label>
    </children>
</fx:root>
