<?xml version="1.0" encoding="UTF-8"?>

<?import com.thefractory.customcomponents.GradientPicker?>
<?import com.thefractory.customcomponents.NumberField?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<StackPane fx:id="controlPanel" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox spacing="5.0">
         <children>
            <Label minWidth="-Infinity" prefWidth="800.0" text="Congratulations! You managed to change tab and are now creating a Hofstadters Butterfly artwork " wrapText="true" />
            <NumberField fx:id="latticePoints" ghostText="Y Coordinate" intField="true" max="10000.0" min="1" onKeyPressed="#changeNumberOfLatP" prefWidth="120.0" />
            <GradientPicker fx:id="gradientPicker" />
         </children>
         <StackPane.margin>
            <Insets />
         </StackPane.margin>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
   </children>
</StackPane>
